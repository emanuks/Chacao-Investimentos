<p id="notice"><%= notice %></p>

<h1>Application Dts</h1>

<table>
  <thead>
    <tr>
      <th>Value</th>
      <th>User</th>
      <th>Direct treasure</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @application_dts.each do |application_dt| %>
      <tr>
        <td><%= application_dt.value %></td>
        <td><%= application_dt.user_id %></td>
        <td><%= application_dt.direct_treasure_id %></td>
        <td><%= link_to 'Show', application_dt %></td>
        <td><%= link_to 'Edit', edit_application_dt_path(application_dt) %></td>
        <td><%= link_to 'Destroy', application_dt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Application Dt', new_application_dt_path %>
